{
  "name": "marein/symfony-standard-headers-csrf-bundle",
  "description": "Protect symfony applications against CSRF attacks with the help of standard headers.",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Markus Reinhold",
      "email": "markusreinhold@icloud.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Marein\\StandardHeadersCsrfBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Marein\\StandardHeadersCsrfBundle\\Tests\\": "tests"
    }
  },
  "require": {
    "php": "^7.4",
    "symfony/http-foundation": "^5.1",
    "symfony/config": "^5.1",
    "symfony/dependency-injection": "^5.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.2",
    "symfony/framework-bundle": "^5.1",
    "symfony/browser-kit": "^5.1",
    "squizlabs/php_codesniffer": "^3.5",
    "phpstan/phpstan": "^0.12.55"
  }
}
