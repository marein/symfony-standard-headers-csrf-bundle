{
  "name": "marein/symfony-standard-headers-csrf-bundle",
  "description": "Protect symfony applications against CSRF attacks with the help of standard headers.",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Markus Reinhold",
      "email": "markusreinhold@icloud.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Marein\\StandardHeadersCsrfBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Marein\\StandardHeadersCsrfBundle\\Tests\\": "tests"
    }
  },
  "require": {
    "php": "^7.4 || ^8.0",
    "symfony/http-foundation": "^5.1 || ^6.0 || ^7.0",
    "symfony/config": "^5.1 || ^6.0 || ^7.0",
    "symfony/dependency-injection": "^5.1 || ^6.0 || ^7.0",
    "symfony/http-kernel": "^5.1 || ^6.0 || ^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "symfony/framework-bundle": "^5.1 || ^6.0 || ^7.0",
    "symfony/browser-kit": "^5.1 || ^6.0 || ^7.0",
    "squizlabs/php_codesniffer": "3.6.1",
    "phpstan/phpstan": "1.2.0"
  }
}
